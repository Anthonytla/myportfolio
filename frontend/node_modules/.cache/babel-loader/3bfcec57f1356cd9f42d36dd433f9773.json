r\n        })\r\n        .catch((e) => {\r\n          console.log(e);\r\n        });\r\n    },\r\n\r\n    newExperience() {\r\n      this.submitted = false;\r\n      this.experience = {};\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.submit-form {\r\n  max-width: 300px;\r\n  margin: auto;\r\n}\r\n</style>"],"sourceRoot":"src/components"}]}