ar data = {\r\n        picture: require('@/assets/' + this.contact.picture),\r\n        title: this.contact.title,\r\n        description: this.contact.description,\r\n        date_created: this.contact.date_created,\r\n        date_updated: this.contact.date_updated,\r\n      };\r\n\r\n      ContactService.create(data)\r\n        .then((response) => {\r\n          this.contact.id = response.data.id;\r\n          console.log(response.data);\r\n          this.submitted = true;\r\n        })\r\n        .catch((e) => {\r\n          console.log(e);\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>"],"sourceRoot":"src/components"}]}