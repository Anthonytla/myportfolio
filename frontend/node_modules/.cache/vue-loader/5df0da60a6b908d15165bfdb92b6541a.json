     <button\r\n            class=\"btn btn-outline-secondary\"\r\n            type=\"button\"\r\n            @click=\"searchTitle\"\r\n          >\r\n            Search\r\n          </button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <div class=\"col-md-6\">\r\n      <h4>Experiences List</h4>\r\n      <ul class=\"list-group\">\r\n        <li\r\n          class=\"list-group-item\"\r\n          :class=\"{ 'col-md-6': index % 2 == 0 }\"\r\n          v-for=\"(experience, index) in experiences\"\r\n          :key=\"index\"\r\n          @click=\"setActiveExperience(experience, index)\"\r\n        >\r\n          {{ experience.title }}\r\n        </li>\r\n      </ul>\r\n\r\n      <button class=\"m-3 btn btn-sm btn-danger\" @click=\"removeAllTutorials\">\r\n        Remove All\r\n      </button>\r\n    </div>\r\n    <div>\r\n      <div v-if=\"currentExperience\">\r\n        <h4>Experience</h4>\r\n        <div>\r\n          <label><strong>Title:</strong></label> {{ currentExperience.title }}\r\n        </div>\r\n        <div>\r\n          <label><strong>Description:</strong></label>\r\n          {{ currentExperience.description }}\r\n        </div>\r\n\r\n        <div>\r\n          <img :src=\"currentExperience.img\">\r\n        </div>\r\n\r\n        <a\r\n          class=\"badge badge-warning\"\r\n          :href=\"'/experience/' + currentExperience._id\"\r\n        >\r\n          Edit\r\n        </a>\r\n        <a class=\"badge badge-warning\" :href=\"'/tasks/' + currentExperience._id\">\r\n          Add task\r\n        </a>\r\n      </div>\r\n      <div v-else>\r\n        <br />\r\n        <p>Please click on a Experience...</p>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport ExperienceService from \"../services/experienceService\";\r\n\r\nexport default {\r\n  name: \"experiences-list\",\r\n  data() {\r\n    return {\r\n      experiences: [],\r\n      currentExperience: null,\r\n      currentIndex: -1,\r\n      title: \"\",\r\n    };\r\n  },\r\n  methods: {\r\n    retrieveExperiences() {\r\n      ExperienceService.getAll()\r\n        .then((response) => {\r\n          this.experiences = response.data;\r\n        })\r\n        .catch((e) => {\r\n          console.log(e);\r\n        });\r\n    },\r\n\r\n    refreshList() {\r\n      this.retrieveTutorials();\r\n      this.currentTutorial = null;\r\n      this.currentIndex = -1;\r\n    },\r\n\r\n    setActiveExperience(experience, index) {\r\n      this.currentExperience = experience;\r\n      this.currentIndex = index;\r\n      console.log(this.currentExperience);\r\n    },\r\n\r\n    removeAllTutorials() {\r\n      ExperienceService.deleteAll()\r\n        .then((response) => {\r\n          console.log(response.data);\r\n          this.refreshList();\r\n        })\r\n        .catch((e) => {\r\n          console.log(e);\r\n        });\r\n    },\r\n\r\n    searchTitle() {\r\n      ExperienceService.findByTitle(this.title)\r\n        .then((response) => {\r\n          this.experiences = response.data;\r\n          console.log(response.data);\r\n        })\r\n        .catch((e) => {\r\n          console.log(e);\r\n        });\r\n    },\r\n  },\r\n  mounted() {\r\n    this.retrieveExperiences();\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.list {\r\n  text-align: left;\r\n  max-width: 750px;\r\n  margin: auto;\r\n}\r\n\r\n.right {\r\n  text-align: right;\r\n}\r\n</style>"]}]}