/>\r\n      </div>\r\n\r\n      <div class=\"form-group\">\r\n        <label for=\"school\">School</label>\r\n        <input\r\n          type=\"string\"\r\n          class=\"form-control\"\r\n          id=\"school\"\r\n          required\r\n          v-model=\"education.school\"\r\n          name=\"school\"\r\n        />\r\n      </div>\r\n\r\n      <div class=\"form-group\">\r\n        <label for=\"degree\">Degree</label>\r\n        <input\r\n          class=\"form-control\"\r\n          id=\"degree\"\r\n          required\r\n          v-model=\"education.degree\"\r\n          name=\"degree\"\r\n        />\r\n      </div>\r\n\r\n      <div class=\"form-group\">\r\n        <label for=\"description\">Description</label>\r\n        <input\r\n          class=\"form-control\"\r\n          id=\"description\"\r\n          required\r\n          v-model=\"education.description\"\r\n          name=\"description\"\r\n        />\r\n      </div>\r\n\r\n      <div class=\"form-group\">\r\n        <label for=\"start_date\">Start date</label>\r\n        <input\r\n          type=\"date\"\r\n          class=\"form-control\"\r\n          id=\"start_date\"\r\n          required\r\n          v-model=\"education.start_date\"\r\n          name=\"start_date\"\r\n        />\r\n      </div>\r\n\r\n      <div class=\"form-group\">\r\n        <label for=\"end_date\">End date</label>\r\n        <input\r\n          type=\"date\"\r\n          class=\"form-control\"\r\n          id=\"end_date\"\r\n          required\r\n          v-model=\"education.end_date\"\r\n          name=\"end_date\"\r\n        />\r\n      </div>\r\n\r\n      <div class=\"form-group\">\r\n        <label for=\"place\">Place</label>\r\n        <input\r\n          type=\"string\"\r\n          class=\"form-control\"\r\n          id=\"place\"\r\n          required\r\n          v-model=\"education.place\"\r\n          name=\"place\"\r\n        />\r\n      </div>\r\n\r\n      <button @click=\"saveEducation\" class=\"btn btn-success\">Submit</button>\r\n    </div>\r\n\r\n    <div v-else>\r\n      <h4>You submitted successfully!</h4>\r\n      <button class=\"btn btn-success\" @click=\"newEducation\">Add</button>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport EducationService from \"../services/educationService\";\r\n\r\nexport default {\r\n  name: \"add-education\",\r\n  data() {\r\n    return {\r\n      education: {\r\n        id: null,\r\n        img: \"\",\r\n        school: \"\",\r\n        degree: \"\",\r\n        description: \"\",\r\n        start_date: \"\",\r\n        end_date: \"\",\r\n        place: \"\",\r\n      },\r\n      submitted: false,\r\n    };\r\n  },\r\n  methods: {\r\n    saveEducation() {\r\n      console.log(this.education.start_date);\r\n      var data = {\r\n        img: require('@/assets/' + this.education.img),\r\n        school: this.education.school,\r\n        degree: this.education.degree,\r\n        description: this.education.description,\r\n        start_date: this.education.start_date,\r\n        end_date: this.education.end_date,\r\n        place: this.education.place,\r\n      };\r\n      console.log(data);\r\n\r\n      EducationService.create(data)\r\n        .then((response) => {\r\n          this.education.id = response.data.id;\r\n          console.log(response.data);\r\n          this.submitted = true;\r\n        })\r\n        .catch((e) => {\r\n          console.log(e);\r\n        });\r\n    },\r\n\r\n    newEducation() {\r\n      this.submitted = false;\r\n      this.education = {};\r\n    },\r\n\r\n    onFileChange(e) {\r\n      var files = e.target.files || e.dataTransfer.files;\r\n      if (!files.length) return;\r\n      this.education.img = e.target.files[0].name;\r\n      console.log(this.education.img);\r\n      // this.createImage(files[0]);\r\n    },\r\n    createImage(file) {\r\n      //var image = new Image();\r\n      var reader = new FileReader();\r\n      var vm = this;\r\n\r\n      reader.onload = (e) => {\r\n        vm.image = e.target.result;\r\n      };\r\n      reader.readAsDataURL(file);\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.submit-form {\r\n  max-width: 300px;\r\n  margin: auto;\r\n}\r\n</style>"]}]}